<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MNEWSAPP.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MNEWSAPP.MVVM.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="MNEWSAPP">

    <ShellContent Route="IndexPage" ContentTemplate="{DataTemplate local:IndexPage}" />
    <ShellContent Route="ArticleDetailsView" ContentTemplate="{DataTemplate local:ArticleDetailsView}" />
    <ShellContent Route="WebPageView" ContentTemplate="{DataTemplate local:WebPageView}" />


</Shell>
